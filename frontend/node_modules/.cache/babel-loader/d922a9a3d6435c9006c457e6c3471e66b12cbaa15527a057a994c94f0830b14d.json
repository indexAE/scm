r\n          <div class=\"gauge-value\">{{ stats.pendingSaleOrders }}</div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"6\">\r\n        <el-card shadow=\"hover\" class=\"gauge-card\">\r\n          <div class=\"gauge-title\">待处理退货</div>\r\n          <div ref=\"returnGauge\" class=\"gauge-chart\"></div>\r\n          <div class=\"gauge-value\">{{ stats.pendingReturns }}</div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"6\">\r\n        <el-card shadow=\"hover\" class=\"gauge-card\">\r\n          <div class=\"gauge-title\">库存预警商品</div>\r\n          <div ref=\"stockGauge\" class=\"gauge-chart\"></div>\r\n          <div class=\"gauge-value\">{{ stats.stockWarnings }}</div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <!-- 模块卡片 -->\r\n    <el-row :gutter=\"20\" style=\"margin-top: 20px\">\r\n      <!-- 基础数据 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><box /></el-icon>\r\n              <span>基础数据</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/basic/supplier')\">\r\n                  <el-icon><user /></el-icon>\r\n                  <span>供应商管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/basic/customer')\">\r\n                  <el-icon><user /></el-icon>\r\n                  <span>客户管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/basic/handler')\">\r\n                  <el-icon><user /></el-icon>\r\n                  <span>经办人管理</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 商品管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><goods /></el-icon>\r\n              <span>商品管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/product/info')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>商品信息</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/product/category')\">\r\n                  <el-icon><folder /></el-icon>\r\n                  <span>分类管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/product/pricing')\">\r\n                  <el-icon><money /></el-icon>\r\n                  <span>定价管理</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 采购管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><shopping-cart /></el-icon>\r\n              <span>采购管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/purchase/requisition')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>采购申请</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/purchase/order')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>采购订单</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/purchase/inbound')\">\r\n                  <el-icon><box /></el-icon>\r\n                  <span>采购入库</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\" style=\"margin-top: 20px\">\r\n      <!-- 销售管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><sell /></el-icon>\r\n              <span>销售管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/sale/order')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>销售订单</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/sale/return')\">\r\n                  <el-icon><refresh-right /></el-icon>\r\n                  <span>退货管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/sale/variance')\">\r\n                  <el-icon><warning /></el-icon>\r\n                  <span>销售差异</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 仓库管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><house /></el-icon>\r\n              <span>仓库管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"12\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/warehouse/info')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>仓库信息</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/warehouse/stock')\">\r\n                  <el-icon><box /></el-icon>\r\n                  <span>库存管理</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 系统管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><setting /></el-icon>\r\n              <span>系统管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/users')\">\r\n                  <el-icon><user /></el-icon>\r\n                  <span>用户管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/operation-log')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>操作日志</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/settings')\">\r\n                  <el-icon><setting /></el-icon>\r\n                  <span>系统设置</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <!-- 趋势图表 -->\r\n    <el-row style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"hover\">\r\n          <div class=\"chart-title\">近30天订单趋势</div>\r\n          <div ref=\"trendChart\" style=\"height: 300px\"></div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport * as echarts from 'echarts'\r\nimport axios from 'axios'\r\nimport { useSettingsStore } from '../stores/settings'\r\nimport {\r\n  ShoppingCart,\r\n  Sell,\r\n  RefreshRight,\r\n  Warning,\r\n  Box,\r\n  User,\r\n  Document,\r\n  Folder,\r\n  Money,\r\n  House,\r\n  Setting\r\n} from '@element-plus/icons-vue'\r\n\r\nconst router = useRouter()\r\nconst username = ref(localStorage.getItem('username') || '用户')\r\nconst settingsStore = useSettingsStore()\r\n\r\nconst stats = ref({\r\n  pendingPurchaseOrders: 0,\r\n  pendingSaleOrders: 0,\r\n  pendingReturns: 0,\r\n  stockWarnings: 0\r\n})\r\n\r\nconst trendChart = ref(null)\r\nconst purchaseGauge = ref(null)\r\nconst saleGauge = ref(null)\r\nconst returnGauge = ref(null)\r\nconst stockGauge = ref(null)\r\n\r\n// 初始化仪表盘\r\nconst initGauges = () => {\r\n  const purchaseChart = echarts.init(purchaseGauge.value)\r\n  const saleChart = echarts.init(saleGauge.value)\r\n  const returnChart = echarts.init(returnGauge.value)\r\n  const stockChart = echarts.init(stockGauge.value)\r\n  \r\n  // 监听窗口大小变化\r\n  window.addEventListener('resize', () => {\r\n    purchaseChart.resize()\r\n    saleChart.resize()\r\n    returnChart.resize()\r\n    stockChart.resize()\r\n  })\r\n  \r\n  return { purchaseChart, saleChart, returnChart, stockChart }\r\n}\r\n\r\n// 更新仪表盘数据\r\nconst updateGauges = (charts, data) => {\r\n  const gaugeOption = (value, max, color) => ({\r\n    series: [{\r\n      type: 'gauge',\r\n      startAngle: 180,\r\n      endAngle: 0,\r\n      min: 0,\r\n      max: max,\r\n      splitNumber: 4,\r\n      itemStyle: {\r\n        color: color\r\n      },\r\n      progress: {\r\n        show: true,\r\n        roundCap: true,\r\n        width: 18\r\n      },\r\n      pointer: {\r\n        show: false\r\n      },\r\n      axisLine: {\r\n        roundCap: true,\r\n        lineStyle: {\r\n          width: 18\r\n        }\r\n      },\r\n      axisTick: {\r\n        show: false\r\n      },\r\n      splitLine: {\r\n        show: false\r\n      },\r\n      axisLabel: {\r\n        show: false\r\n      },\r\n      title: {\r\n        show: false\r\n      },\r\n      detail: {\r\n        show: false\r\n      },\r\n      data: [{\r\n        value: value\r\n      }]\r\n    }]\r\n  })\r\n\r\n  charts.purchaseChart.setOption(gaugeOption(data.pendingPurchaseOrders, 10, '#409EFF'))\r\n  charts.saleChart.setOption(gaugeOption(data.pendingSaleOrders, 10, '#67C23A'))\r\n  charts.returnChart.setOption(gaugeOption(data.pendingReturns, 20, '#E6A23C'))\r\n  charts.stockChart.setOption(gaugeOption(data.stockWarnings, 20, '#F56C6C'))\r\n}\r\n\r\n// 获取数据\r\nconst fetchData = async (charts) => {\r\n  try {\r\n    // 获取统计数据\r\n    const statsRes = await axios.get('http://localhost:8080/api/dashboard/scm-stats')\r\n    if (statsRes.data.code === 0) {\r\n      stats.value = statsRes.data.data\r\n      updateGauges(charts, stat