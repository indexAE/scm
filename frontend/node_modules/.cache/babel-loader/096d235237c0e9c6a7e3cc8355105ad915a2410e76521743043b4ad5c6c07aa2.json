urn Box;\n      },\n      get User() {\n        return User;\n      },\n      get Document() {\n        return Document;\n      },\n      get Folder() {\n        return Folder;\n      },\n      get Money() {\n        return Money;\n      },\n      get House() {\n        return House;\n      },\n      get Setting() {\n        return Setting;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","echarts","axios","useSettingsStore","ShoppingCart","Sell","RefreshRight","Warning","Box","User","Document","Folder","Money","House","Setting","router","username","localStorage","getItem","settingsStore","stats","pendingPurchaseOrders","pendingSaleOrders","pendingReturns","stockWarnings","trendChart","purchaseGauge","saleGauge","returnGauge","stockGauge","initCharts","charts","purchaseChart","init","value","saleChart","returnChart","stockChart","window","addEventListener","Object","values","forEach","chart","resize","fetchData","_ref2","_asyncToGenerator","_regeneratorRuntime","mark","_callee","statsRes","trendRes","wrap","_callee$","_context","prev","next","get","sent","data","code","updateGauges","updateTrendChart","t0","console","error","stop","_x","apply","arguments","option","tooltip","trigger","legend","xAxis","type","dates","yAxis","series","name","purchaseOrders","saleOrders","returnOrders","setOption","gaugeOption","max","colors","radius","startAngle","endAngle","pointer","show","progress","overlap","roundCap","clip","itemStyle","color","x","y","x2","y2","colorStops","offset","axisLine","lineStyle","width","splitLine","axisTick","axisLabel","title","fontSize","offsetCenter","detail","height","formatter","getInstanceByDom"],"sources":["C:/Users/indexae/Desktop/scm退货/scm/frontend/src/views/Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <!-- 欢迎语 -->\r\n    <div class=\"welcome-section\">\r\n      <h2>欢迎回来, {{ username }}</h2>\r\n      <p class=\"system-desc\">{{ settingsStore.systemDesc }}</p>\r\n    </div>\r\n\r\n    <!-- 核心统计数据 -->\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"6\">\r\n        <el-card shadow=\"hover\" class=\"data-card\">\r\n          <div class=\"data-item\">\r\n            <el-icon class=\"icon\"><shopping-cart /></el-icon>\r\n            <div class=\"data-content\">\r\n              <div class=\"data-title\">待处理采购订单</div>\r\n              <div class=\"data-value\">{{ stats.pendingPurchaseOrders }}</div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"6\">\r\n        <el-card shadow=\"hover\" class=\"data-card\">\r\n          <div class=\"data-item\">\r\n            <el-icon class=\"icon\"><sell /></el-icon>\r\n            <div class=\"data-content\">\r\n              <div class=\"data-title\">待处理销售订单</div>\r\n              <div class=\"data-value\">{{ stats.pendingSaleOrders }}</div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"6\">\r\n        <el-card shadow=\"hover\" class=\"data-card\">\r\n          <div class=\"data-item\">\r\n            <el-icon class=\"icon\"><refresh-right /></el-icon>\r\n            <div class=\"data-content\">\r\n              <div class=\"data-title\">待处理退货</div>\r\n              <div class=\"data-value\">{{ stats.pendingReturns }}</div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      <el-col :span=\"6\">\r\n        <el-card shadow=\"hover\" class=\"data-card\">\r\n          <div class=\"data-item\">\r\n            <el-icon class=\"icon\"><warning /></el-icon>\r\n            <div class=\"data-content\">\r\n              <div class=\"data-title\">库存预警商品</div>\r\n              <div class=\"data-value\">{{ stats.stockWarnings }}</div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <!-- 模块卡片 -->\r\n    <el-row :gutter=\"20\" style=\"margin-top: 20px\">\r\n      <!-- 基础数据 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><box /></el-icon>\r\n              <span>基础数据</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/basic/supplier')\">\r\n                  <el-icon><user /></el-icon>\r\n                  <span>供应商管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/basic/customer')\">\r\n                  <el-icon><user /></el-icon>\r\n                  <span>客户管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/basic/handler')\">\r\n                  <el-icon><user /></el-icon>\r\n                  <span>经办人管理</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 商品管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><goods /></el-icon>\r\n              <span>商品管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/product/info')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>商品信息</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/product/category')\">\r\n                  <el-icon><folder /></el-icon>\r\n                  <span>分类管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/product/pricing')\">\r\n                  <el-icon><money /></el-icon>\r\n                  <span>定价管理</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 采购管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><shopping-cart /></el-icon>\r\n              <span>采购管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/purchase/requisition')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>采购申请</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/purchase/order')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>采购订单</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/purchase/inbound')\">\r\n                  <el-icon><box /></el-icon>\r\n                  <span>采购入库</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\" style=\"margin-top: 20px\">\r\n      <!-- 销售管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><sell /></el-icon>\r\n              <span>销售管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/sale/order')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>销售订单</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/sale/return')\">\r\n                  <el-icon><refresh-right /></el-icon>\r\n                  <span>退货管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/sale/variance')\">\r\n                  <el-icon><warning /></el-icon>\r\n                  <span>销售差异</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 仓库管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><house /></el-icon>\r\n              <span>仓库管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"12\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/warehouse/info')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>仓库信息</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/warehouse/stock')\">\r\n                  <el-icon><box /></el-icon>\r\n                  <span>库存管理</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 系统管理 -->\r\n      <el-col :span=\"8\">\r\n        <el-card class=\"module-card\" shadow=\"hover\">\r\n          <template #header>\r\n            <div class=\"module-header\">\r\n              <el-icon><setting /></el-icon>\r\n              <span>系统管理</span>\r\n            </div>\r\n          </template>\r\n          <div class=\"module-content\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/users')\">\r\n                  <el-icon><user /></el-icon>\r\n                  <span>用户管理</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/operation-log')\">\r\n                  <el-icon><Document /></el-icon>\r\n                  <span>操作日志</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"8\">\r\n                <div class=\"quick-entry\" @click=\"router.push('/dashboard/settings')\">\r\n                  <el-icon><setting /></el-icon>\r\n                  <span>系统设置</span>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <!-- 趋势图表 -->\r\n    <el-row style=\"margin-top: 20px\">\r\n      <el-col :span=\"24\">\r\n        <el-card shadow=\"hover\">\r\n          <div class=\"chart-title\">近30天订单趋势</div>\r\n          <div ref=\"trendChart\" style=\"height: 300px\"></div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport * as echarts from 'echarts'\r\nimport axios from 'axios'\r\nimport { useSettingsStore } from '../stores/settings'\r\nimport {\r\n  ShoppingCart,\r\n  Sell,\r\n  RefreshRight,\r\n  Warning,\r\n  Box,\r\n  User,\r\n  Document,\r\n  Folder,\r\n  Money,\r\n  House,\r\n  Setting\r\n} from '@element-plus/icons-vue'\r\n\r\nconst router = useRouter()\r\nconst username = ref(localStorage.getItem('username') || '用户')\r\nconst settingsStore = useSettingsStore()\r\n\r\nconst stats = ref({\r\n  pendingPurchaseOrders: 0,\r\n  pendingSaleOrders: 0,\r\n  pendingReturns: 0,\r\n  stockWarnings: 0\r\n})\r\n\r\nconst trendChart = ref(null)\r\nconst purchaseGauge = ref(null)\r\nconst saleGauge = ref(null)\r\nconst returnGauge = ref(null)\r\nconst stockGauge = ref(null)\r\n\r\n// 初始化图表\r\nconst initCharts = () => {\r\n  const charts = {\r\n    purchaseChart: echarts.init(purchaseGauge.value),\r\n    saleChart: echarts.init(saleGauge.value),\r\n    returnChart: echarts.init(returnGauge.value),\r\n    stockChart: echarts.init(stockGauge.value),\r\n    trendChart: echarts.init(trendChart.value)\r\n  }\r\n  \r\n  // 监听窗口大小变化\r\n  window.addEventListener('resize', () => {\r\n    Object.values(charts).forEach(chart => chart.resize())\r\n  })\r\n  \r\n  fetchData(charts)\r\n}\r\n\r\n// 获取数据\r\nconst fetchData = async (charts) => {\r\n  try {\r\n    // 获取统计数据\r\n    const statsRes = await axios.get('http://localhost:8080/api/dashboard/scm-stats')\r\n    if (statsRes.data.code === 0) {\r\n      stats.value = statsRes.data.data\r\n      updateGauges(charts, statsRes.data.data)\r\n    }\r\n\r\n    // 获取订单趋势数据\r\n    const trendRes = await axios.get('http://localhost:8080/api/dashboard/trend')\r\n    if (trendRes.data.code === 0) {\r\n      updateTrendChart(charts.trendChart, trendRes.data.data)\r\n    }\r\n  } catch (error) {\r\n    console.error('获取数据失败:', error)\r\n  }\r\n}\r\n\r\n// 更新订单趋势图表\r\nconst updateTrendChart = (chart, data) => {\r\n  const option = {\r\n    tooltip: {\r\n      trigger: 'axis'\r\n    },\r\n    legend: {\r\n      data: ['采购订单', '销售订单', '退货订单']\r\n    },\r\n    xAxis: {\r\n      type: 'category',\r\n      data: data.dates\r\n    },\r\n    yAxis: {\r\n      type: 'value'\r\n    },\r\n    series: [\r\n      {\r\n        name: '采购订单',\r\n        type: 'line',\r\n        data: data.purchaseOrders\r\n      },\r\n      {\r\n        name: '销售订单',\r\n        type: 'line',\r\n        data: data.saleOrders\r\n      },\r\n      {\r\n        name: '退货订单',\r\n        type: 'line',\r\n        data: data.returnOrders\r\n      }\r\n    ]\r\n  }\r\n  chart.setOption(option)\r\n}\r\n\r\n// 更新仪表盘数据\r\nconst updateGauges = (charts, data) => {\r\n  const gaugeOption = (value, max, colors) => ({\r\n    series: [{\r\n      type: 'gauge',\r\n      radius: '100%',\r\n      startAngle: 90,\r\n      endAngle: -270,\r\n      pointer: {\r\n        show: false\r\n      },\r\n      progress: {\r\n        show: true,\r\n        overlap: false,\r\n        roundCap: true,\r\n        clip: false,\r\n        itemStyle: {\r\n          color: {\r\n            type: 'linear',\r\n            x: 0,\r\n            y: 0,\r\n            x2: 0,\r\n            y2: 1,\r\n            colorStops: [{\r\n              offset: 0,\r\n              color: colors[0]\r\n            }, {\r\n              offset: 1,\r\n              color: colors[1]\r\n            }]\r\n          }\r\n        }\r\n      },\r\n      axisLine: {\r\n        lineStyle: {\r\n          width: 18,\r\n          color: [[1, '#E1E1E1']]\r\n        }\r\n      },\r\n      splitLine: {\r\n        show: false\r\n      },\r\n      axisTick: {\r\n        show: false\r\n    