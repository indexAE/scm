  }]);\n\n    // 路由路径与中文标题的映射\n    var routeTitleMap = {\n      'users': '用户管理',\n      'roles': '角色管理',\n      'notice': '公告通知',\n      'list': '通知列表',\n      'publish': '发布通知',\n      'basic': '基础数据',\n      'supplier': '供应商管理',\n      'customer': '客户管理',\n      'dealers': '经销商管理',\n      'retailers': '零售商管理',\n      'purchaser': '采购员管理',\n      'handler': '经办人管理',\n      'product': '商品管理',\n      'info': '商品信息',\n      'category': '分类管理',\n      'pricing': '定价管理',\n      'purchase': '采购管理',\n      'requisition': '采购申请',\n      'order': '订单管理',\n      'inbound': '采购入库',\n      'sale': '销售管理',\n      'return': '退货管理',\n      'variance': '销售差异',\n      'warehouse': '仓库管理',\n      'stock': '库存管理',\n      'operation-log': '操作日志',\n      'login-log': '登录日志',\n      'settings': '系统设置',\n      'profile': '个人信息'\n    };\n\n    // 计算面包屑项\n    var breadcrumbItems = computed(function () {\n      var paths = route.path.split('/').filter(Boolean);\n      paths.shift(); // 移除 'dashboard'\n\n      return paths.map(function (path) {\n        return {\n          path: \"/\".concat(paths.slice(0, paths.indexOf(path) + 1).join('/')),\n          title: routeTitleMap[path] || path\n        };\n      });\n    });\n\n    // 计算当前激活的菜单项\n    var activeMenu = computed(function () {\n      return route.path;\n    });\n\n    // 处理下拉菜单命令\n    var handleCommand = function handleCommand(command) {\n      if (command === 'logout') {\n        localStorage.removeItem('username');\n        localStorage.removeItem('userId');\n        router.push('/login');\n        ElMessage.success('已退出登录');\n      } else if (command === 'profile') {\n        router.push('/dashboard/profile');\n      }\n    };\n\n    // 处理菜单选择\n    var handleSelect = function handleSelect(index) {\n      router.push(index);\n    };\n\n    // 添加新的标签页\n    var addView = function addView(route) {\n      var path = route.path;\n      if (visitedViews.value.some(function (v) {\n        return v.path === path;\n      })) return;\n      var title = routeTitleMap[path.split('/').pop()] || '未知页面';\n      visitedViews.value.push({\n        path: path,\n        title: title\n      });\n      activeTab.value = path;\n    };\n\n    // 关闭标签页\n    var handleTabRemove = function handleTabRemove(targetPath) {\n      var tabs = visitedViews.value;\n      if (activeTab.value === targetPath) {\n        tabs.forEach(function (tab, index) {\n          if (tab.path === targetPath) {\n            var nextTab = tabs[index + 1] || tabs[index - 1];\n            if (nextTab) {\n              activeTab.value = nextTab.path;\n              router.push(nextTab.path);\n            }\n          }\n        });\n      }\n      visitedViews.value = tabs.filter(function (tab) {\n        return tab.path !== targetPath;\n      });\n    };\n\n    // 点击标签页\n    var handleTabClick = function handleTabClick(tab) {\n      router.push(tab.props.name);\n    };\n\n    // 监听路由变化\n    watch(function () {\n      return route.path;\n    }, function (path) {\n      if (path === '/dashboard') {\n        activeTab.value = path;\n        return;\n      }\n      addView(route);\n    }, {\n      immediate: true\n    });\n\n    // 在组件挂载时获取系统设置\n    onMounted(function () {\n      // settingsStore.fetchSettings()\n    });\n    var __returned__ = {\n      router: router,\n      route: route,\n      settingsStore: settingsStore,\n      username: username,\n      isCollapse: isCollapse,\n      activeTab: activeTab,\n      visitedViews: visitedViews,\n      routeTitleMap: routeTitleMap,\n      breadcrumbItems: breadcrumbItems,\n      activeMenu: activeMenu,\n      handleCommand: handleCommand,\n      handleSelect: handleSelect,\n      addView: addView,\n      handleTabRemove: handleTabRemove,\n      handleTabClick: handleTabClick,\n      ref: ref,\n      computed: computed,\n      onMounted: onMounted,\n      watch: watch,\n      get useRouter() {\n        return useRouter;\n      },\n      get useRoute() {\n        return useRoute;\n      },\n      get ArrowDown() {\n        return ArrowDown;\n      },\n      get House() {\n        return House;\n      },\n      get User() {\n        return User;\n      },\n      get Setting() {\n        return Setting;\n      },\n      get DocumentIcon() {\n        return DocumentIcon;\n      },\n      get Box() {\n        return Box;\n      },\n      get ShoppingCart() {\n        return ShoppingCart;\n      },\n      get Goods() {\n        return Goods;\n      },\n      get Sell() {\n        return Sell;\n      },\n      get Document() {\n        return Document;\n      },\n      get RefreshRight() {\n        return RefreshRight;\n      },\n      get Warning() {\n        return Warning;\n      },\n      get Fold() {\n        return Fold;\n      },\n      get Expand() {\n        return Expand;\n      },\n      get Bell() {\n        return Bell;\n      },\n      get Money() {\n        return Money;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get useSettingsStore() {\n        return useSettingsStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","useRouter","useRoute","ArrowDown","House","User","Setting","Document","DocumentIcon","Box","ShoppingCart","Goods","Sell","RefreshRight","Warning","Fold","Expand","Bell","Money","ElMessage","useSettingsStore","router","route","settingsStore","username","localStorage","getItem","isCollapse","activeTab","visitedViews","path","title","routeTitleMap","breadcrumbItems","paths","split","filter","Boolean","shift","map","concat","slice","indexOf","join","activeMenu","handleCommand","command","removeItem","push","success","handleSelect","index","addView","value","some","v","pop","handleTabRemove","targetPath","tabs","forEach","tab","nextTab","handleTabClick","props","name","immediate"],"sources":["C:/Users/indexae/Desktop/scm退货/scm/frontend/src/views/Dashboard.vue"],"sourcesContent":["<template>\n  <el-container class=\"layout-container\">\n    <!-- 顶部导航栏 -->\n    <el-header>\n      <div class=\"header-left\">\n        <el-icon \n          class=\"collapse-btn\"\n          @click=\"isCollapse = !isCollapse\"\n        >\n          <fold v-if=\"!isCollapse\"/>\n          <expand v-else/>\n        </el-icon>\n        <h2>{{ settingsStore.systemName }}</h2>\n        <el-breadcrumb separator=\"/\" class=\"breadcrumb\">\n          <el-breadcrumb-item :to=\"{ path: '/dashboard' }\">首页</el-breadcrumb-item>\n          <el-breadcrumb-item v-for=\"(item, index) in breadcrumbItems\" :key=\"index\">\n            {{ item.title }}\n          </el-breadcrumb-item>\n        </el-breadcrumb>\n      </div>\n      <div class=\"header-right\">\n        <el-dropdown @command=\"handleCommand\">\n          <span class=\"el-dropdown-link\">\n            {{ username }}<el-icon class=\"el-icon--right\"><arrow-down /></el-icon>\n          </span>\n          <template #dropdown>\n            <el-dropdown-menu>\n              <el-dropdown-item command=\"profile\">个人信息</el-dropdown-item>\n              <el-dropdown-item command=\"logout\">退出登录</el-dropdown-item>\n            </el-dropdown-menu>\n          </template>\n        </el-dropdown>\n      </div>\n    </el-header>\n    \n    <!-- 标签页导航 -->\n    <div class=\"tabs-container\">\n      <el-tabs\n        v-model=\"activeTab\"\n        type=\"card\"\n        closable\n        @tab-click=\"handleTabClick\"\n        @tab-remove=\"handleTabRemove\"\n        class=\"nav-tabs\"\n      >\n        <el-tab-pane\n          v-for=\"item in visitedViews\"\n          :key=\"item.path\"\n          :label=\"item.title\"\n          :name=\"item.path\"\n          :closable=\"item.path !== '/dashboard'\"\n        >\n        </el-tab-pane>\n      </el-tabs>\n    </div>\n\n    <el-container>\n      <!-- 左侧菜单栏 -->\n      <el-aside :width=\"isCollapse ? '64px' : '200px'\" class=\"sidebar-container\">\n        <el-menu\n          :default-active=\"activeMenu\"\n          class=\"el-menu-vertical\"\n          @select=\"handleSelect\"\n          :router=\"true\"\n          :collapse=\"isCollapse\"\n        >\n          <el-menu-item index=\"/dashboard\">\n            <el-icon><house /></el-icon>\n            <template #title>首页</template>\n          </el-menu-item>\n\n          <el-sub-menu index=\"2\">\n            <template #title>\n              <el-icon><user /></el-icon>\n              <span>用户管理</span>\n            </template>\n            <el-menu-item index=\"/dashboard/users\">用户列表</el-menu-item>\n            <el-menu-item index=\"/dashboard/roles\">角色管理</el-menu-item>\n          </el-sub-menu>\n\n          <el-sub-menu index=\"3\">\n            <template #title>\n              <el-icon><bell /></el-icon>\n              <span>公告通知</span>\n            </template>\n            <el-menu-item index=\"/dashboard/notice/list\">通知列表</el-menu-item>\n            <el-menu-item index=\"/dashboard/notice/publish\">发布通知</el-menu-item>\n          </el-sub-menu>\n\n          <el-sub-menu index=\"4\">\n            <template #title>\n              <el-icon><box /></el-icon>\n              <span>基础数据</span>\n            </template>\n            <el-menu-item index=\"/dashboard/basic/supplier\">供应商管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/basic/customer\">客户管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/basic/dealers\">经销商管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/basic/retailers\">零售商管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/basic/purchaser\">采购员管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/basic/handler\">经办人管理</el-menu-item>\n          </el-sub-menu>\n\n          <!-- 商品管理 -->\n          <el-sub-menu index=\"5\">\n            <template #title>\n              <el-icon><goods /></el-icon>\n              <span>商品管理</span>\n            </template>\n            <el-menu-item index=\"/dashboard/product/info\">商品信息</el-menu-item>\n            <el-menu-item index=\"/dashboard/product/category\">分类管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/product/pricing\">定价管理</el-menu-item>\n          </el-sub-menu>\n\n          <!-- 采购管理 -->\n          <el-sub-menu index=\"6\">\n            <template #title>\n              <el-icon><shopping-cart /></el-icon>\n              <span>采购管理</span>\n            </template>\n            <el-menu-item index=\"/dashboard/purchase/requisition\">采购申请</el-menu-item>\n            <el-menu-item index=\"/dashboard/purchase/order\">采购订单</el-menu-item>\n            <el-menu-item index=\"/dashboard/purchase/inbound\">采购入库</el-menu-item>\n          </el-sub-menu>\n\n          <!-- 销售管理 -->\n          <el-sub-menu index=\"7\">\n            <template #title>\n              <el-icon><Sell /></el-icon>\n              <span>销售管理</span>\n            </template>\n            <el-menu-item index=\"/dashboard/sale/order\">\n              <el-icon><Document /></el-icon>\n              <span>销售订单</span>\n            </el-menu-item>\n            <el-menu-item index=\"/dashboard/sale/return\">\n              <el-icon><RefreshRight /></el-icon>\n              <span>退货管理</span>\n            </el-menu-item>\n            <el-menu-item index=\"/dashboard/sale/variance\">\n              <el-icon><Warning /></el-icon>\n              <span>销售差异</span>\n            </el-menu-item>\n          </el-sub-menu>\n\n          <!-- 仓库管理 -->\n          <el-sub-menu index=\"8\">\n            <template #title>\n              <el-icon><house /></el-icon>\n              <span>仓库管理</span>\n            </template>\n            <el-menu-item index=\"/dashboard/warehouse/info\">仓库信息</el-menu-item>\n            <el-menu-item index=\"/dashboard/warehouse/stock\">库存管理</el-menu-item>\n          </el-sub-menu>\n\n          <!-- 资金管理 -->\n          <el-sub-menu index=\"9\">\n            <template #title>\n              <el-icon><money /></el-icon>\n              <span>资金管理</span>\n            </template>\n            <el-menu-item index=\"/dashboard/finance/income\">收入管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/finance/expense\">支出管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/finance/account\">账户管理</el-menu-item>\n            <el-menu-item index=\"/dashboard/finance/report\">财务报表</el-menu-item>\n          </el-sub-menu>\n\n          <el-sub-menu index=\"10\">\n            <template #title>\n              <el-icon><document-icon /></el-icon>\n              <span>日志管理</span>\n            </template>\n            <el-menu-item index=\"/dashboard/operation-log\">操作日志</el-menu-item>\n            <el-menu-item index=\"/dashboard/login-log\">登录日志</el-menu-item>\n          </el-sub-menu>\n\n          <el-menu-item index=\"/dashboard/settings\">\n            <el-icon><setting /></el-icon>\n            <span>系统设置</span>\n          </el-menu-item>\n        </el-menu>\n      </el-aside>\n      \n      <!-- 主内容区域 -->\n      <el-main>\n        <router-view></router-view>\n      </el-main>\n    </el-container>\n  </el-container>\n</template>\n\n<script setup>\nimport { ref, computed, onMounted, watch } from 'vue'\nimport { useRouter, useRoute } from 'vue-router'\nimport { \n  ArrowDown, \n  House, \n  User, \n  Setting, \n  Document as DocumentIcon, \n  Box,\n  ShoppingCart,\n  Goods,\n  Sell,\n  Document,\n  RefreshRight,\n  Warning,\n  Fold,\n  Expand,\n  Bell,\n  Money\n} from '@element-plus/icons-vue'\nimport { ElMessage } from 'element-plus'\nimport { useSettingsStore } from '../stores/settings'\n\nconst router = useRouter()\nconst route = useRoute()\nconst settingsStore = useSettingsStore()\nconst username = ref(localStorage.getItem('username') || '用户')\nconst isCollapse = ref(false)\nconst activeTab = ref('/dashboard')\nconst visitedViews = ref([\n  { path: '/dashboard', title: '首页' }\n])\n\n// 路由路径与中文标题的映射\nconst routeTitleMap = {\n  'users': '用户管理',\n  'roles': '角色管理',\n  'notice': '公告通知',\n  'list': '通知列表',\n  'publish': '发布通知',\n  'basic': '基础数据',\n  'supplier': '供应商管理',\n  'customer': '客户管理',\n  'dealers': '经销商管理',\n  'retailers': '零售商管理',\n  'purchaser': '采购员管理',\n  'handler': '经办人管理',\n  'product': '商品管理',\n  'info': '商品信息',\n  'category': '分类管理',\n  'pricing': '定价管理',\n  'purchase': '采购管理',\n  'requisition': '采购申请',\n  'order': '订单管理',\n  'inbound': '采购入库',\n  'sale': '销售管理',\n  'return': '退货管理',\n  'variance': '销售差异',\n  'warehouse': '仓库管理',\n  'stock': '库存管理',\n  'operation-log': '操作日志',\n  'login-log': '登录日志',\n  'settings': '系统设置',\n  'profile': '个人信息'\n}\n\n// 计算面包屑项\nconst breadcrumbItems = computed(() => {\n  const paths = route.path.split('/').filter(Boolean)\n  paths.shift() // 移除 'dashboard'\n  \n  return paths.map(path => ({\n    path: `/${paths.slice(0, paths.indexOf(path) + 1).join('/')}`,\n    title: routeTitleMap[path] || path\n  }))\n})\n\n// 计算当前激活的菜单项\nconst activeMenu = computed(() => route.path)\n\n// 处理下拉菜单命令\nconst handleCommand = (command) => {\n  if (command === 'logout') {\n    localStorage.removeItem('username')\n    localStorage.removeItem('userId')\n    router.push('/login')\n    ElMessage.success('已退出登录')\n  } else if (command === 'profile') {\n    router.push('/dashboard/profile')\n  }\n}\n\n// 处理菜单选择\nconst handleSelect = (index) => {\n  router.push(index)\n}\n\n// 添加新的标签页\nconst addView = (route) => {\n  const { path } = route\n  if (visitedViews.value.some(v => v.path === path)) return\n  \n  const title = routeTitleMap[path.split('/').pop()] || '未知页面'\n  visitedViews.value.push({\n    path,\n    title\n  })\n  activeTab.value = path\n}\n\n// 关闭标签页\nconst handleTabRemove = (targetPath) => {\n  const tabs = visitedViews.value\n  \n  if (activeTab.value === targetPath) {\n    tabs.forEach((tab, index) => {\n      if (tab.path === targetPath) {\n        const nextTab = tabs[index + 1] || tabs[index - 1]\n        if (nextTab) {\n          activeTab.value = nextTab.path\n          router.push(nextTab.path)\n        }\n      }\n    })\n  }\n  \n  visitedViews.value = tabs.filter(tab => tab.path !== targetPath)\n}\n\n// 点击标签页\nconst handleTabClick = (tab) => {\n  router.push(tab.props.name)\n}\n\n// 监听路由变化\nwatch(\n  () => route.path,\n  (path) => {\n    if (path === '/dashboard') {\n      activeTab.value = path\n      return\n    }\n    addView(route)\n  },\n  { immediate: true }\n)\n\n// 在组件挂载时获取系统设置\nonMounted(() => {\n  // settingsStore.fetchSettings()\n})\n</script>\n\n<style scoped>\n.layout-container {\n  height: 100vh;\n}\n\n.el-header {\n  background-color: var(--header-background);\n  color: white;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 20px;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  z-index: 1000;\n}\n\n.header-left {\n  display: flex;\n  align-items: center;\n  flex: 1;\n}\n\n.header-left h2 {\n  margin: 0;\n  margin-right: 20px;\n  white-space: nowrap;\n  font-size: 20px;\n  font-weight: bold;\n}\n\n.el-dropdown-link {\n  color: white;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n}\n\n.el-aside {\n  background-color: var(--menu-background);\n  border-right: 1px solid var(--border-color);\n  position: fixed;\n  top: 60px;\n  bottom: 0;\n  left: 0;\n  z-index: 900;\n  overflow-y: auto;\n  max-height: calc(100vh - 60px);\n  transition: width 0.3s;\n  \n  /* 美化滚动条 */\n  &::-webkit-scrollbar {\n    width: 6px;\n  }\n  \n  &::-webkit-scrollbar-thumb {\n    background: rgba(144, 147, 153, 0.3);\n    border-radius: 3px;\n  }\n  \n  &::-webkit-scrollbar-track {\n    background: transparent;\n  }\n  \n  /* 平滑滚动 */\n  scroll-behavior: smooth;\n}\n\n.el-menu {\n  border-right: none;\n  background-color: var(--menu-background);\n}\n\n.el-menu-vertical {\n  height: 100%;\n  overflow-y: auto;\n  \n  /* 美化滚动条 */\n  &::-webkit-scrollbar {\n    width: 6px;\n  }\n  \n  &::-webkit-scrollbar-thumb {\n    background: rgba(144, 147, 153, 0.3);\n    border-radius: 3px;\n  }\n  \n  &::