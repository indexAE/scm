pdown>\n      </div>\n    </el-header>\n    \n    <el-container>\n      <!-- 左侧菜单栏 -->\n      <el-aside width=\"200px\">\n        <el-menu\n          default-active=\"1\"\n          class=\"el-menu-vertical\"\n          @select=\"handleSelect\"\n        >\n          <el-menu-item index=\"1\">\n            <el-icon><house /></el-icon>\n            <span>首页</span>\n          </el-menu-item>\n          <el-sub-menu index=\"2\">\n            <template #title>\n              <el-icon><user /></el-icon>\n              <span>用户管理</span>\n            </template>\n            <el-menu-item index=\"2-1\">用户列表</el-menu-item>\n            <el-menu-item index=\"2-2\">角色管理</el-menu-item>\n          </el-sub-menu>\n          <el-sub-menu index=\"3\">\n            <template #title>\n              <el-icon><Box /></el-icon>\n              <span>基础数据</span>\n            </template>\n            <el-menu-item index=\"3-1\">供应商管理</el-menu-item>\n            <el-menu-item index=\"3-2\">客户管理</el-menu-item>\n          </el-sub-menu>\n          <el-menu-item index=\"4\">\n            <el-icon><setting /></el-icon>\n            <span>系统设置</span>\n          </el-menu-item>\n          <el-sub-menu index=\"5\">\n            <template #title>\n              <el-icon><Document /></el-icon>\n              <span>日志管理</span>\n            </template>\n            <el-menu-item index=\"5-1\">操作日志</el-menu-item>\n            <el-menu-item index=\"5-2\">登录日志</el-menu-item>\n          </el-sub-menu>\n        </el-menu>\n      </el-aside>\n      \n      <!-- 主内容区域 -->\n      <el-main>\n        <router-view></router-view>\n        <div class=\"welcome\" v-if=\"$route.path === '/dashboard' && settingsStore.showWelcome\">\n          <h2>欢迎回来, {{ username }}</h2>\n          <p class=\"system-desc\">{{ settingsStore.systemDesc }}</p>\n        </div>\n      </el-main>\n    </el-container>\n  </el-container>\n</template>\n\n<script setup>\nimport { ref, onMounted } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { ArrowDown, House, User, Setting, Document, Box } from '@element-plus/icons-vue'\nimport { ElMessage } from 'element-plus'\nimport { useSettingsStore } from '../stores/settings'\n\nconst router = useRouter()\nconst settingsStore = useSettingsStore()\nconst username = ref(localStorage.getItem('username') || '用户')\n\n// 处理下拉菜单命令\nconst handleCommand = (command) => {\n  if (command === 'logout') {\n    // 清除所有用户信息\n    localStorage.removeItem('username')\n    localStorage.removeItem('userId')\n    router.push('/login')\n    ElMessage.success('已退出登录')\n  } else if (command === 'profile') {\n    router.push('/dashboard/profile')\n  }\n}\n\n// 处理菜单选择\nconst handleSelect = (index) => {\n  switch (index) {\n    case '1':\n      router.push('/dashboard')\n      break\n    case '2-1':\n      router.push('/dashboard/users')\n      break\n    case '2-2':\n      router.push('/dashboard/roles')\n      break\n    case '3-1':\n      router.push('/dashboard/suppliers')\n      break\n    case '3-2':\n      router.push('/dashboard/customers')\n      break\n    case '4':\n      router.push('/dashboard/settings')\n      break\n    case '5-1':\n      router.push('/dashboard/operation-log')\n      break\n    case '5-2':\n      router.push('/dashboard/login-log')\n      break\n  }\n}\n\n// 在组件挂载时获取系统设置\nonMounted(() => {\n  settingsStore.fetchSettings()\n})\n</script>\n\n<style scoped>\n.layout-container {\n  height: 100vh;\n}\n\n.el-header {\n  background-color: #409eff;\n  color: white;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 20px;\n}\n\n.header-left h2 {\n  margin: 0;\n}\n\n.el-dropdown-link {\n  color: white;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n}\n\n.el-menu-vertical {\n  height: 100%;\n  border-right: none;\n}\n\n.el-main {\n  background-color: #f5f5f5;\n  padding: 20px;\n}\n\n.welcome {\n  text-align: center;\n  padding: 40px 20px;\n  background-color: white;\n  border-radius: 8px;\n  box-shad